<%- include('./partials/header.ejs', {title:"Browse"})%>

<body class="wine-index">
    
    <h1>Browse zee wines</h1>
    <ul>
        <% wines.forEach((wine) => { %>
            <figure id="wineTile"> <a href="/wines/<%=wine.id%>"><img id="wine" src="<%=wine.thumb%>"> <figcaption><%=wine.name%></figcaption> </a> </figure>
        <% }) %>
    </ul>
    
</body>
</html>